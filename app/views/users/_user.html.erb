<div id="<%= dom_id user %>">
  <p>
    <strong>Name:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Birthday:</strong>
    <%= user.birthday %>
  </p>

  <div>
    <strong>User Picture:</strong>
    <%= image_tag(url_for(user.profile_image)) %>
  </div>
  <br>
  <br>
</div>
